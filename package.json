{
  "name": "nodemaker",
  "version": "1.0.0",
  "description": "Quick proof of concept for n8n node generator",
  "main": "index.js",
  "scripts": {
    "start": "npm run empty && npm run nodegen && npm run packgen",
    "nodegen": "cp .hygen.js ./build/.hygen.js && tsc && node build/generateNodeFiles.js --nodeGenerationType complex",
    "packgen": "tsc && node build/generatePackageJson.js",
    "place": "tsc && node build/placeFiles.js",
    "empty": "rm -rfv output/*"
  },
  "keywords": ["n8n"],
  "author": "Iv√°n Ovejero",
  "license": "MIT",
  "dependencies": {
    "hygen": "^5.0.3",
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "@types/node": "^14.0.20",
    "@types/node-fetch": "^2.5.7",
    "typescript": "^3.9.6"
  }
}
