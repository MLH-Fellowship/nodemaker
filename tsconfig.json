{
	"exclude": [ "output" ],
	"include": [
		"*.ts",
		"*.hygen.ts",
		"*.d.ts"
  ],
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"outDir": "./build",
		"strict": true,
		"strictPropertyInitialization": false,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true
	}
}
