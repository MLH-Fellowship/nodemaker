{
	"exclude": [ "output" ],
	"include": [
		"**/*.ts",
		"*.d.ts", "client/src/store/modules/fields.js"
  ],
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"outDir": "./build",
		"strict": true,
		"strictPropertyInitialization": false,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true
	}
}
